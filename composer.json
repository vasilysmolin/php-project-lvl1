{
    "name": "hexlet/code",
    "description": "hexlet library",
    "type": "library",
    "keywords": [
        "php"
    ],
    "bin": [
        "bin/brain-games",
        "bin/brain-even",
        "bin/brain-calc",
        "bin/brain-gcd",
        "bin/brain-progression",
        "bin/brain-prime"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "vasily.smolin",
            "email": "vasilysmoolin@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "src/Cli.php",
            "src/Engine.php",
            "src/Games/Calc.php",
            "src/Games/Even.php",
            "src/Games/Gcd.php",
            "src/Games/Prime.php",
            "src/Games/Progression.php"
        ]
    },
    "scripts": {
        "phpcs": "phpcs --standard=PSR2"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2.0",
        "wp-cli/php-cli-tools": "^0.11.11"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5"
    }
}
